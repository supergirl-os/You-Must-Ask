  font-size: 30rpx;
  justify-content:center; 
  align-items:center;
}  
.stepper{
  height:60rpx;
  margin:20rpx 0 0 0;
  width:60rpx;
  padding:0;
  border-radius: 20rpx 0 0rpx 20rpx;
  border-color: grey;
  background: #99abe1;
  border-style: solid;
  border-width: 2rpx; 
}
.steppero{
  height:60rpx;
  margin:20rpx 0 0 0;
  width:60rpx;
  padding:0;
  border-radius: 0rpx 20rpx 20rpx 0rpx;
  border-width: 2rpx;
  border-color: grey;
  background: #99abe1;
  border-style: solid;
}

 
/*普通样式*/  
.stepper .normal{  
    color: black;  
}  
  
/*禁用样式*/  
.stepper .disabled{  
    color: #ccc;  
}  

